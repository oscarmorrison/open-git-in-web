echo "Opening git repo"
giturl=$(git config --get remote.origin.url)
giturl=$( tr '[A-Z]' '[a-z]' <<< $giturl)

if [[ $giturl == *"http"* ]]
then
	echo "Opening HTTP"
  open $giturl
else
	giturl=${giturl/\.git}
	giturl=${giturl//[:]/\/}
	giturl=${giturl/\git@}
	protocol="http://"
	giturl="$protocol$giturl"
	echo "Opening SSH Git"
	open $giturl
fi


